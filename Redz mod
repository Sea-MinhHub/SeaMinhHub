loadstring(game:HttpGet("https://sea-minh-protect.vercel.app/api/raw/3sy9cfr00hdk"))()
-- anti afk

if getconnections then
    for _, c in ipairs(getconnections(game.Players.LocalPlayer.Idled)) do
        c:Disable()
    end
    print("[AntiAFK] Đã bật, bạn sẽ không bị kick AFK.")
else
    print("[AntiAFK] Executor không hỗ trợ getconnections.")
end
